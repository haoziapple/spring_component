<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 生成命令：mvn mybatis-generator:generate -->

	<!-- 数据库驱动包位置 -->
	<classPathEntry
		location="C:\Users\Administrator\.m2\repository\mysql\mysql-connector-java\5.1.39" />

	<context id="genMappers" targetRuntime="MyBatis3">
		<commentGenerator>
			<!-- 阻止生成注释，默认false -->
			<property name="suppressAllComments" value="false" />
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<!-- 连接配置 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://10.108.26.11:3306/fz_aquatic_zf" userId="fz_aquatic_zf"
			password="fuzhong2015" />

		<!-- 是否强制DECIMAL和NUMERIC类型的字段转换为BigDecimal,默认值为false -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.haozi.demo.springboot.bean.po"
			targetProject="src/main/java" />

		<!-- 映射文件的包名和位置 -->
		<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources" />

		<!-- DAO接口类 -->
		<!-- ANNOTATEDMAPPER:基于注解的Mapper接口，不会有对应的XML映射文件 -->
		<!-- XMLMAPPER:所有的方法都在XML中，接口调用依赖XML文件 -->
		<javaClientGenerator targetPackage="com.haozi.demo.springboot.dao"
			targetProject="src/main/java" type="XMLMAPPER" />

		<!-- 不生成example方法 -->
		<table tableName="%" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
	</context>
</generatorConfiguration>  